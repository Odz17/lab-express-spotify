<div class="tracks-container">
  {{#each tracks}}
    <div class="track">
      <h2>{{name}} / Remastered</h2>
      <div class="audio-container">
        <audio controls>
          <source src="{{preview_url}}" type="audio/mpeg" />
          Your browser does not support the audio tag.
        </audio>
      </div>
    </div>
  {{/each}}
</div>
